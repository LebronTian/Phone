body {
	background-color: #F2F2F2;
	min-width: 1200px;
}

.ellipsis {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}


/*满屏高度至底部*/

.fullpage {
	height: 100%;
}


/*超出限定隐藏overflow:hidden*/

.overflow-h {
	overflow: hidden;
}

.main {
	padding-bottom: 71px;
}


/*使元素页面高度超出100%样式出现滚动条*/

.main-left .tab-content,
.main-center .app-view,
.main-right>.tabbable .tab-content {
	overflow-y: auto;
	height: 100%;
	padding-bottom: 40px;
}

.scrollbar-style::-webkit-scrollbar {
	/*滚动条整体样式*/
	width: 2px;
	/*高宽分别对应横竖滚动条的尺寸*/
	height: 4px;
}

.scrollbar-style::-webkit-scrollbar-thumb {
	/*滚动条里面小方块*/
	border-radius: 1px;
	-webkit-box-shadow: inset 0 0 1px rgba(0, 0, 0, 0.2);
	background: rgba(0, 0, 0, 0.2);
}

.scrollbar-style::-webkit-scrollbar-track {
	/*滚动条里面轨道*/
	-webkit-box-shadow: inset 0 0 1px rgba(0, 0, 0, 0.2);
	border-radius: 0;
	background: rgba(0, 0, 0, 0.1);
}


