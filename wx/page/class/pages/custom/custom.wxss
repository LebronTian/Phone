/*新闻样式*/
.xcx_news {
	min-height: 50px;
	height: auto;
	overflow: hidden;
	background-color: #fff;
}
.xcx_news .news {
	height: 100px;
	width: 100%;
	border-bottom: 1px #f5f5f5 solid;
}
.xcx_news .news .lt {
	width: 68%;
	height: 88px;
	float: left;
	margin-left: 2%;
	margin-top: 5px;
	position: relative;
}
.xcx_news .news .title {
	width: 100%;
	height: 25px;
	font-size: 15px;
	font-weight: bold;
	line-height: 23px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	color: #000;
}
.xcx_news .news .intro {
	width: 100%;
	height: 35px;
	line-height: 17px;
	text-overflow: ellipsis;
	overflow: hidden;
	font-size: 12px;
	color: #515151;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	display: -webkit-box !important;
	word-break: break-all;
}
.xcx_news .news .info {
	position: absolute;
	bottom: 0px;
}
.xcx_news .news .info text {
	font-size: 12px;
	color: #515151;
	margin-right: 10px;
}
.xcx_news .news .info text {
	font-size: 12px;
	color: #515151;
	margin-right: 10px;
}
.xcx_news .news .rt {
	width: 24%;
	height: 90px;
	float: left;
	margin: 5px 2%;
}
.xcx_news .news .rt image {
	width: 100%;
	height: 100%;
}


